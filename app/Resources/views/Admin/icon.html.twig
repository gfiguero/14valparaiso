{% block iconos %}
    {% if app.request.attributes.get( '_route' ) starts with 'page' %}{{ icon('clone fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'product' %}{{ icon('cube fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'notice' %}{{ icon('newspaper-o fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'member' %}{{ icon('users fa-fw') }}
    {% elseif app.request.attributes.get( '_route' ) starts with 'document' %}{{ icon('file fa-fw') }}
    {% endif %}
{% endblock %}
